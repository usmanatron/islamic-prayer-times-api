{
  "root": "/var/www/html",
  "temp": "/tmp",
  "public": "/var/www/html/public",
  "error_handler_middleware": {
    "display_error_details": true,
    "log_errors": true,
    "log_error_details": true
  },
  "prayer_time_defaults": {
    "method": "ISNA",
    "midnight_mode": "Standard",
    "latitude_adjustment_method": "None",
    "elevation": 0,
    "school": "Standard"
  },
  "logger": {
    "name": "ipt-api",
    "path": "/var/log/ipt-api.log",
    "include_stdout": true,
    "debug": true
  }
}
